<h1>Profiles#new</h1>
<p>Find me in app/views/profiles/new.html.erb</p>

<%= form_for :profile, url: profiles_create_path do |f| %>
	<p>
		<%= f.label :name %>
		<%= f.text_field :name %>
	</p>
	<p>
		<%= f.label :bio %>
		<%= f.text_field :bio %>
	</p>
	<p>
		<%= f.label :image %>
		<%= f.file_field :image %>
	</p>
	<%= f.submit %>
<% end %>